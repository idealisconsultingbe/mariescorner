<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <report
        id="action_report_mc_delivery_slip"
        string="[MC] Delivery Slip"
        model="stock.picking"
        report_type="qweb-pdf"
        file="mc_stock_layout.report_deliveryslip"
        name="mc_stock_layout.report_deliveryslip"
        print_report_name="'Delivery Slip - %s - %s' % (object.partner_id.name or '', object.name)"
    />

    <report
        id="action_report_mc_fictitious_delivery_slip"
        string="[MC] Fictitious Delivery Slip"
        model="stock.picking"
        report_type="qweb-pdf"
        file="mc_stock_layout.report_fictitious_deliveryslip"
        name="mc_stock_layout.report_fictitious_deliveryslip"
        print_report_name="'Delivery Slip - %s - %s' % (object.partner_id.name or '', object.name)"
    />

    <report
        id="action_report_mc_cmr_document"
        string="CMR Document"
        model="stock.picking"
        report_type="qweb-pdf"
        file="mc_stock_layout.report_cmr"
        name="mc_stock_layout.report_cmr"
        paperformat="mc_stock_layout.cmr_paperformat_a4"
        print_report_name="'CMR - %s - %s' % (object.partner_id.name or '', object.name)"
    />

</odoo>
